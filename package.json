{
  "name": "redis-ui",
  "version": "1.0.0",
  "description": "",
  "main": "build/lib/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run build && npm run build-scss && npm run electron",
    "electron": "electron .",
    "build": "npm run build-folders && npm run build-js && npm run build-pages && npm run build-scss",
    "build-folders": "mkdir -p build/lib/pages && mkdir -p build/lib/styles",
    "build-js": "babel src -d build/lib",
    "build-pages": "cp src/pages/index.html build/lib/pages",
    "build-scss": "node-sass src/styles/main.scss --include-path node_modules/foundation-sites/scss > build/lib/styles/main.css"
  },
