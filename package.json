  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "electron": "^1.3.5",
    "node-sass": "^3.8.0",
    "uglify-js": "^2.7.3"
  },
  "dependencies": {
    "foundation-sites": "^6.2.3",
    "jquery": "^2.2.4",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-actions": "^0.12.0",
    "redux-saga": "^0.11.1"
  }
